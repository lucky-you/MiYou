<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">


        <com.zhowin.base_library.view.TitleView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mTitle="联系我们" />

        <View
            style="@style/DivideLineStyle"
            android:layout_height="10dp"
            android:background="@color/color_fafafa" />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:divider="@drawable/shape_divide_line_bg"
            app:dividerPadding="16dp"
            app:showDividers="middle">

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/RelativeLayoutWidth44DpStyle"
                android:layout_height="50dp">

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="微信公众号"
                    android:textColor="@color/color_333"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="@string/app_name"
                    android:textColor="@color/color_343434"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/RelativeLayoutWidth44DpStyle"
                android:layout_height="50dp">

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="支付宝生活号"
                    android:textColor="@color/color_333"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="zfb123456"
                    android:textColor="@color/color_343434"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/RelativeLayoutWidth44DpStyle"
                android:layout_height="50dp">

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="抖音号ID/昵称"
                    android:textColor="@color/color_333"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="my13245231"
                    android:textColor="@color/color_343434"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/RelativeLayoutWidth44DpStyle"
                android:layout_height="50dp">

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="官方网站"
                    android:textColor="@color/color_333"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="http://fadfafa.com"
                    android:textColor="@color/color_343434"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/RelativeLayoutWidth44DpStyle"
                android:layout_height="50dp">

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="官方微信"
                    android:textColor="@color/color_333"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="http://fadfafa.com"
                    android:textColor="@color/color_343434"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                style="@style/RelativeLayoutWidth44DpStyle"
                android:layout_height="50dp">

                <TextView
                    style="@style/TextViewStyleOf14Sp"
                    android:gravity="center_vertical"
                    android:text="在线客服"
                    android:textColor="@color/color_333"
                    android:textSize="15sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/mine_more_icon"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.appcompat.widget.LinearLayoutCompat>


        <View
            style="@style/DivideLineStyle"
            android:layout_height="10dp"
            android:background="@color/color_fafafa" />


    </LinearLayout>

</layout>